{
  "name": "@tt-bot-dev/web",
  "version": "1.0.0",
  "description": "The web dashboard for tt.bot",
  "main": "dist/server/index.mjs",
  "type": "module",
  "scripts": {
    "build::server": "tsc -p tsconfig.json",
    "build::client": "parcel build --dist-dir dist-client --public-url '/static/' --no-scope-hoist --no-source-maps './lib/client/bundle/*'",
    "build::all": "npm run build::server && npm run build::client",
    "lint": "eslint --ext .js,.ts .",
    "lint::fix": "eslint --ext .js,.ts --fix .",
    "install": "npm run build::all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://owo.codes/tt.bot/modules/web.git"
  },
  "keywords": [
    "tt.bot",
    "web",
    "dashboard",
    "api"
  ],
  "author": "TTtie <me@tttie.cz>",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@parcel/optimizer-cssnano": "^2.2.1",
    "@parcel/packager-css": "^2.2.1",
    "@parcel/transformer-css": "^2.2.1",
    "@parcel/transformer-postcss": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "@polka/redirect": "^1.0.0-next.0",
    "@polka/send-type": "^0.5.2",
    "@tt-bot-dev/types": "git+https://owo.codes/tt.bot/modules/types.git",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-signature": "^1.0.3",
    "@types/csurf": "^1.11.2",
    "@types/ejs": "^3.1.0",
    "@types/express-session": "^1.17.4",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.15",
    "@types/polka": "^0.5.4",
    "@types/uuid": "^8.3.4",
    "body-parser": "^1.19.1",
    "bulma": "^0.9.3",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "cookie-signature": "^1.1.0",
    "csurf": "git+https://owo.codes/tt.bot/external/csurf.git",
    "discord-api-types": "^0.26.1",
    "ejs": "^3.1.6",
    "express-session": "^1.17.2",
    "parcel": "^2.2.1",
    "polka": "^0.5.2",
    "postcss": "^8.4.6",
    "sass": "^1.49.7",
    "sosamba": "git+https://owo.codes/tt.bot/frameworks/sosamba.git#v2.0.0",
    "typescript": "^4.6.0-dev.20220205",
    "undici": "^4.13.0",
    "uuid": "^8.3.2"
  },
  "targets": {
    "main": false
  },
  "devDependencies": {
    "@tt-bot-dev/eslint-config": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": ">=7.20.0"
  }
}
